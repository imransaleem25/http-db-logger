{
    "name": "imransaleem/http-db-logger",
    "description": "Middleware to log HTTP requests and responses to database",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Imran Saleem",
            "email": "imran.saleem2425@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Imransaleem\\HttpDbLogger\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Imransaleem\\HttpDbLogger\\HttpDbLoggerServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
